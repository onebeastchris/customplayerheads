{
  "schemaVersion": 1,
  "id": "customplayerheads",
  "version": "${version}",
  "name": "CustomPlayerHeads",
  "description": "A mod that drops player heads. But with support for skin change mods (e.g. FabricTailor), and Geyser players!",
  "authors": [
    "onebeastofchris"
  ],
  "contact": {
    "homepage": "https://github.com/onebeastchris/customplayerheads",
    "sources": "https://github.com/onebeastchris/customplayerheads"
  },
  "license": "MIT",
  "icon": "assets/customplayerheads/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.onebeastofchris.customplayerheads.CustomPlayerHeads"
    ]
  },
  "mixins": [
    "customplayerheads.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.15.10",
    "fabric-api": "*",
    "minecraft": ">=1.20.5",
    "java": ">=21"
  },
  "suggests": {
    "another-mod": "*"
  }
}